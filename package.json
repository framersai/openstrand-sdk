{
  "name": "@framers/openstrand-sdk",
  "version": "0.1.1",
  "description": "TypeScript SDK for the OpenStrand knowledge graph APIs.",
  "author": "Framers <team@frame.dev>",
  "license": "MIT",
  "copyright": "Copyright (c) 2025 Framers (https://frame.dev)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "require": "./dist/types/index.js",
      "import": "./dist/types/index.mjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "assets"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "format": "node ../../scripts/run-prettier.mjs --write",
    "format:check": "node ../../scripts/run-prettier.mjs --check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "docs": "typedoc",
    "release": "semantic-release",
    "postinstall": "husky install",
    "commitlint": "commitlint"
  },
  "dependencies": {
    "@framers/sql-storage-adapter": "^0.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^24.0.0",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.13",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,yml,yaml,css,scss}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.8.1"
  },
  "keywords": [
    "openstrand",
    "pkms",
    "knowledge-management",
    "sdk",
    "typescript",
    "api-client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/framersai/openstrand-monorepo",
    "directory": "packages/openstrand-sdk"
  },
  "bugs": {
    "url": "https://github.com/framersai/openstrand-monorepo/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "homepage": "https://openstrand.ai"
}
